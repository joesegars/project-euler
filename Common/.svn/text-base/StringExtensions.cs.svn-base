using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace Common
{
  public static class StringExtensions
  {
    public static bool IsAtLeastFiveCharactersLong(this string value)
    {
      return value.Length >= 5;
    }

    public static bool IsExactlyFiveCharactersLong(this string value)
    {
      return value.Length == 5;
    }
  }
}
